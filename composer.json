{
    "name": "workflowable/workflowable",
    "description": "A workflow engine for Laravel",
    "keywords": [
        "Workflow Engine",
        "laravel",
        "workflowable"
    ],
    "homepage": "https://github.com/workflowable/workflowable",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Leach",
            "email": "7387639+andyleach@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "code-stencil/code-stencil": ">=1",
        "illuminate/contracts": "^v10.18.0",
        "spatie/laravel-package-tools": "^1.16.0",
        "workflowable/forms": "dev-master"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.6.1",
        "orchestra/testbench": "^8.6",
        "pestphp/pest": "^v2.13",
        "pestphp/pest-plugin-arch": "^2.2.3",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-phpunit": "^1.3.13",
        "spatie/laravel-ray": "^1.32.6"
    },
    "autoload": {
        "psr-4": {
            "Workflowable\\Workflowable\\": "src/",
            "Workflowable\\Workflowable\\Database\\Factories\\": "database/factories",
            "Workflowable\\Workflowable\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Workflowable\\Workflowable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "analyze": "vendor/bin/phpstan analyze",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "style": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workflowable\\Workflowable\\WorkflowableServiceProvider"
            ],
            "aliases": {
                "Workflowable": "Workflowable\\Workflowable\\Facades\\Workflowable"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../workflow-forms"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
